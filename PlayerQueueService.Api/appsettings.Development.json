{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Telemetry": {
    "ServiceName": "player-queue-service.api",
    "MeterName": "player-queue-service.metrics",
    "ServiceVersion": "0.0.8",
    "Metrics": {
      "Endpoint": "http://localhost:4318/v1/metrics",
      "Protocol": "http"
    },
    "Tracing": {
      "Endpoint": "http://localhost:4318/v1/traces",
      "Protocol": "http"
    }
  },
  "Matchmaking": {
    "TeamSize": 2,
    "MaxSkillDelta": 200,
    "MaxQueueSeconds": 120
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": null,
    "Password": null,
    "VirtualHost": "/",
    "QueueName": "player-queue.enqueued",
    "ExchangeName": "player-queue",
    "RoutingKey": "player.queue.enqueued",
    "MatchResultsExchangeName": "player-match",
    "MatchResultsQueueName": "player-match.formed",
    "MatchResultsRoutingKey": "player.match.formed",
    "PrefetchCount": 5,
    "HeartbeatSeconds": 60,
    "MaxRetryAttempts": 3,
    "RetryDelaySeconds": 2,
    "PublishConfirmTimeoutSeconds": 5
  }
}
