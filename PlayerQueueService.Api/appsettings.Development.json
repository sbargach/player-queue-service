{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Telemetry": {
    "ServiceName": "player-queue-service.api",
    "Metrics": {
      "Endpoint": "http://localhost:4318/v1/metrics",
      "Protocol": "http"
    },
    "Tracing": {
      "Endpoint": "http://localhost:4318/v1/traces",
      "Protocol": "http"
    }
  },
  "Matchmaking": {
    "TeamSize": 2,
    "MaxSkillDelta": 200,
    "MaxQueueSeconds": 120
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": null,
    "Password": null,
    "VirtualHost": "/",
    "QueueName": "player-queue.enqueued",
    "ExchangeName": "player-queue",
    "RoutingKey": "player.queue.enqueued",
    "DeadLetterExchangeName": "player-queue.dlx",
    "DeadLetterQueueName": "player-queue.dead-letter",
    "DeadLetterRoutingKey": "player.queue.dead-letter",
    "MatchResultsExchangeName": "player-match",
    "MatchResultsQueueName": "player-match.formed",
    "MatchResultsRoutingKey": "player.match.formed",
    "PrefetchCount": 5,
    "HeartbeatSeconds": 60,
    "MaxRetryAttempts": 3,
    "RetryDelaySeconds": 2,
    "PublishConfirmTimeoutSeconds": 5
  }
}
